CAPI=2:
name: invap:rts:aurora_regmap:0.1.0
description: rdl frame decoder

filesets:
  rdl:
    files:
        - aurora_regmap.rdl : {file_type: user }
    depend:
        - ~stratum:utils:rdl_gen:0.2

generate:
  rdl_regmap:
    generator: rdl_gen
    parameters:
      rdl_file: aurora_regmap.rdl
      documentation:
      headers:
        - verilog

targets:
  default: &default
    filesets:
      - rdl
    generate:
      - rdl_regmap